
# Cloudflare R2 Storage
CLOUDFLARE_R2_ENABLED=true  # Set to 'true' to enable Cloudflare R2 storage
CLOUDFLARE_R2_BUCKET=your-bucket-name  # Your Cloudflare R2 bucket name
CLOUDFLARE_R2_PUBLIC_URL=your-public-bucket-url  # e.g. pub-xxxxxx.r2.dev (without https://)
CLOUDFLARE_R2_BUCKET_ENDPOINT=your-bucket-endpoint-url  # e.g. https://xxxxxx.r2.cloudflarestorage.com
CLOUDFLARE_R2_ACCESS_KEY=your-access-key  # Your Cloudflare R2 access key
CLOUDFLARE_R2_SECRET_KEY=your-secret-key  # Your Cloudflare R2 secret key
CLOUDFLARE_R2_ACCOUNT_ID=your-account-id  # Your Cloudflare account ID

# PostgreSQL database settings
POSTGRES_DB=your-db-name  # Your PostgreSQL database name
POSTGRES_USER=your-db-user  # Your PostgreSQL username
POSTGRES_PASSWORD=your-db-password  # Your PostgreSQL password
POSTGRES_HOST=localhost  # Database host (default: localhost)
POSTGRES_PORT=5432  # Database port (default: 5432)